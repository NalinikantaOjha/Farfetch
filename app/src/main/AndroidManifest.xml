<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.farfetch">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Farfetch">
        <activity android:name="Activity.CardActivity"></activity>
        <activity android:name="Activity.PaymentActivity" />
        <activity android:name="Fragment.FragmentActivity3" />
        <activity android:name="Fragment.FragmentActivity2" />
        <activity android:name="Fragment.FragmentActivity" />
        <activity android:name="Activity.SignUpActivity" />
        <activity android:name="Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>